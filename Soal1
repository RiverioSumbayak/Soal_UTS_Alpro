#include <iostream>
#include <string>
using namespace std;

bool hanya_angka(const string& s) {
    for (char c : s) {
        if (!isdigit(c)) return false;
    }
    return true;
}

string tipe_kartu(const string& s) {
    if (s.length() == 14 && s.substr(0, 2) == "65")
        return "NUSANTARA";
    else if (s.length() == 16 && 
            (s.substr(0, 2) == "77" || s.substr(0, 2) == "78"))
        return "GARUDA";
    else if (s.length() == 15 && s.substr(0, 2) == "91")
        return "MERDEKA";
    return "TIDAK DIKENAL";
}

bool validasi(const string& s) {
    int total = 0;
    bool gandakan = false;

    for (int i = s.length() - 1; i >= 0; i--) {
        int digit = s[i] - '0';
        if (gandakan) {
            digit *= 2;
            if (digit > 9)
                digit -= 9;
        }
        total += digit;
        gandakan = !gandakan;
    }

    return total % 10 == 0;
}

int main() {
    string kartu;
    cout << "Masukkan nomor kartu digital: ";
    cin >> kartu;

    if (!hanya_angka(kartu)) {
        cout << "Nomor kartu harus hanya terdiri dari angka!" << endl;
        return 1;
    }

    string tipe = tipe_kartu(kartu);

    if (tipe == "TIDAK DIKENAL") {
        cout << "Jenis kartu tidak dikenali!" << endl;
        return 1;
    }

    cout << "Jenis Kartu: " << tipe << endl;

    if (validasi(kartu)) {
        cout << "Kartu valid." << endl;
    } else {
        cout << "Kartu TIDAK valid." << endl;
    }

    return 0;
}
